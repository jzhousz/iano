<?xml version="1.0" encoding="UTF-8"?>
<Algorithms>
	<Algorithm type="2DExtractor">
		<Name>Wavelet</Name>
		<Desc>2D discrete wavelet decomposition</Desc>
		<Parameter type="Integer">
			<Name>level</Name>
			<Low>1</Low>
			<High>10</High>
			<Default>2</Default>
		</Parameter>
	</Algorithm>
	<Algorithm type="2DExtractor">
		<Name>Principal Components</Name>
		<Parameter type="Boolean">
			<Name>Reduction</Name>
			<Default>0</Default>
		</Parameter>
	</Algorithm>
	<Algorithm type="Selector">
		<Name>mRMR-MIQ</Name>
		<Parameter type="Integer">
			<Name>feature selected</Name>
			<Default>10</Default>
		</Parameter>
	</Algorithm>
	<Algorithm type="Selector">
		<Name>mRMR-MID</Name>
		<Parameter type="Integer">
			<Name>feature selected</Name>
			<Default>10</Default>
		</Parameter>
	</Algorithm>
	<Algorithm type="2DExtractor">
		<Name>HAAR</Name>
		<Parameter type="Integer">
			<Name>Wavelet Level</Name>
			<Default>2</Default>
		</Parameter>
	</Algorithm>
	<Algorithm type="Classifier">
		<Name>SVM</Name>
		<Desc>Support Vector Machine</Desc>
		<Parameter type="String">
			<Name>General Parameter</Name>
			<Default>-s 0</Default>
		</Parameter>
	</Algorithm>
	<Algorithm type="Classifier">
		<Name>Tree</Name>
	</Algorithm>
	<Algorithm type="Classifier">
		<Name>Naive Bayes</Name>
	</Algorithm>
	<Algorithm type="Classifier">
		<Name>Random Forest</Name>
	</Algorithm>
	<Algorithm type="Classifier">
		<Name>Nearest Neighbor</Name>
		<Parameter type="Integer">
			<Name>K</Name>
			<Low>1</Low>
			<High>10</High>
			<Default>3</Default>
		</Parameter>
	</Algorithm>
</Algorithms>
